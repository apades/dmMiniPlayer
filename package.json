{
  "name": "share-gpt",
  "version": "1.0.0",
  "license": "MIT",
  "author": "apades",
  "packageManager": "pnpm@7.29.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "test:danmaku": "sh -c \"tsup \"./src/test/danmaku/${0}.ts\" -d ./dist --format esm && node \"./dist/${0}.mjs\" ${1}\""
  },
  "devDependencies": {
    "@types/chrome": "^0.0.231",
    "@types/color": "^3.0.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "18.15.11",
    "@types/pako": "^2.0.0",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@types/ws": "^8.5.5",
    "cross-env": "^7.0.3",
    "prettier": "2.8.7",
    "protobufjs": "^7.2.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "@apad/env-tools": "^1.0.5",
    "@chakra-ui/react": "^2.6.0",
    "@plasmohq/messaging": "^0.3.3",
    "@plasmohq/storage": "^1.5.0",
    "ass-parser": "^0.2.0",
    "bilibili-live-ws": "^6.3.1",
    "color": "^4.2.3",
    "lodash-es": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "pako": "^2.1.0",
    "plasmo": "0.68.1",
    "python-struct": "^1.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^5.2.0",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0",
    "ws": "^8.13.0"
  },
  "manifest": {
    "name": "弹幕画中画播放器",
    "description": "带弹幕的画中画播放器，目前已支持B站和斗鱼，CC直播，狠狠的摸！",
    "version": "0.2.9",
    "host_permissions": [
      "*://*.bilibili.com/*",
      "*://api.cc.163.com/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/**/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "commands": {
      "back": {
        "suggested_key": {
          "windows": "Alt+Shift+Comma",
          "mac": "Command+Shift+Left"
        },
        "description": "后退"
      },
      "forward": {
        "suggested_key": {
          "windows": "Alt+Shift+Period",
          "mac": "Command+Shift+Right"
        },
        "description": "快进"
      },
      "pause/play": {
        "suggested_key": {
          "windows": "Alt+Shift+M",
          "mac": "Command+Shift+Space"
        },
        "description": "暂停/播放"
      },
      "hide": {
        "suggested_key": {
          "windows": "Ctrl+Shift+H",
          "mac": "Command+Shift+H"
        },
        "description": "隐藏画中画"
      },
      "playbackRate": {
        "description": "开启/关闭倍速，默认3倍速，修改请到设置页"
      }
    }
  }
}
