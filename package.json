{
  "name": "share-gpt",
  "version": "1.0.0",
  "license": "MIT",
  "author": "apades",
  "scripts": {
    "dev": "vite",
    "build": "node ./scripts/build.mjs",
    "test": "sh ./scripts/run-test.sh",
    "release": "node ./scripts/release.mjs"
  },
  "devDependencies": {
    "@apad/env-tools": "^1.0.5",
    "@apad/vite-plugin": "2.0.1",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@types/chrome": "^0.0.231",
    "@types/color": "^3.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "18.15.11",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "archiver": "^7.0.1",
    "cross-env": "^7.0.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.1.1",
    "less": "^4.2.0",
    "lodash-es": "^4.17.21",
    "prettier": "2.8.7",
    "protobufjs": "^7.2.3",
    "tailwindcss": "^3.3.5",
    "typescript": "5.1.6",
    "vite": "^5.2.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@apad/rc-slider": "^10.5.0",
    "@apad/setting-panel": "^0.1.8",
    "ahooks": "^3.7.8",
    "ass-compiler": "^0.1.11",
    "bilibili-live-ws": "^6.3.1",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "events": "^3.3.0",
    "fast-deep-equal": "^3.1.3",
    "mitt": "^3.0.1",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "pako": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "manifest": {
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "author": "apades",
    "manifest_version": 3,
    "version": "0.4.2",
    "icons": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "48": "assets/icon48.png",
      "64": "assets/icon64.png",
      "128": "assets/icon128.png"
    },
    "action": {
      "default_icon": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "64": "assets/icon64.png",
        "128": "assets/icon128.png"
      },
      "default_popup": "src/popup/index.html"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "scripting"
    ],
    "background": {
      "service_worker": "./src/background/index.ts"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "./src/contents/clogInject.ts"
        ],
        "run_at": "document_end",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "./src/contents/floatButton.ts"
        ],
        "run_at": "document_end",
        "all_frames": true
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "./src/contents/main.ts"
        ],
        "run_at": "document_end"
      },
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "./src/contents/main.ts"
        ],
        "run_at": "document_end",
        "world": "MAIN"
      }
    ],
    "default_locale": "en",
    "web_accessible_resources": [
      {
        "resources": [
          "assets/**/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "commands": {
      "back": {
        "suggested_key": {
          "default": "Alt+Shift+Comma",
          "windows": "Alt+Shift+Comma",
          "mac": "Command+Shift+Left"
        },
        "description": "__MSG_back__"
      },
      "forward": {
        "suggested_key": {
          "default": "Alt+Shift+Period",
          "windows": "Alt+Shift+Period",
          "mac": "Command+Shift+Right"
        },
        "description": "__MSG_forward__"
      },
      "pause/play": {
        "suggested_key": {
          "default": "Alt+Shift+M",
          "windows": "Alt+Shift+M",
          "mac": "Command+Shift+Space"
        },
        "description": "__MSG_playOrPause__"
      },
      "hide": {
        "suggested_key": {
          "default": "Alt+Shift+H",
          "windows": "Alt+Shift+H",
          "mac": "Command+Shift+H"
        },
        "description": "__MSG_hide__"
      },
      "playbackRate": {
        "description": "__MSG_playbackRate__"
      }
    }
  }
}
