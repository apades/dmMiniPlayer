  "scripts": {
    "dev": "plasmo dev",
    "build": "node ./scripts/build.mjs",
    "package": "plasmo package",
    "test": "vite serve -c ./src/test/vite.config.js ./src/test"
  },
  "devDependencies": {
    "@apad/env-tools": "^1.0.5",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@types/chrome": "^0.0.231",
    "@types/classnames": "^2.3.1",
    "@types/color": "^3.0.3",
    "@types/node": "18.15.11",
    "@types/pako": "^2.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1",
    "less": "^4.2.0",
    "prettier": "2.8.7",
    "protobufjs": "^7.2.3",
    "typescript": "5.1.6",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@apad/injector": "^0.0.3",
    "@apad/setting-panel": "^0.0.17",
    "@plasmohq/messaging": "^0.3.3",
    "@plasmohq/storage": "^1.5.0",
    "ahooks": "^3.7.8",
    "antd": "4.16.6",
    "ass-parser": "^0.2.0",
    "bilibili-live-ws": "^6.3.1",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "fast-deep-equal": "^3.1.3",
    "mitt": "^3.0.1",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "pako": "^2.1.0",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0"
  },
  "manifest": {
    "name": "弹幕画中画播放器",
    "description": "支持最新的画中画API功能，且在b站、斗鱼可以播放/发送弹幕",
    "version": "0.3.5",
    "host_permissions": [
      "<all_urls>"
    ],
    "default_locale": "zh_CN",
    "web_accessible_resources": [
      {
        "resources": [
          "assets/**/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "commands": {
      "back": {
        "suggested_key": {
          "default": "Alt+Shift+Comma",
          "windows": "Alt+Shift+Comma",
          "mac": "Command+Shift+Left"
        },
        "description": "后退"
      },
      "forward": {
        "suggested_key": {
          "default": "Alt+Shift+Period",
          "windows": "Alt+Shift+Period",
          "mac": "Command+Shift+Right"
        },
        "description": "快进"
      },
      "pause/play": {
        "suggested_key": {
          "default": "Alt+Shift+M",
          "windows": "Alt+Shift+M",
          "mac": "Command+Shift+Space"
        },
        "description": "暂停/播放"
      },
      "hide": {
        "suggested_key": {
          "default": "Alt+Shift+H",
          "windows": "Alt+Shift+H",
          "mac": "Command+Shift+H"
        },
        "description": "隐藏画中画"
      },
      "playbackRate": {
        "description": "开启/关闭倍速，默认3倍速，修改请到设置页"
      }
    }
  }
}
